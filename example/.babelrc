{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "safari >= 7"
          ],
          "node": "6.10"
        }
      }
    ],
    "react",
    "stage-2"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel",
        "react-flow-props-to-prop-types"
      ]
    },
    "test": {
      "plugins": [
        "react-flow-props-to-prop-types"
      ]
    }
  }
}
